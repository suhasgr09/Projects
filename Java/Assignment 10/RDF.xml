      <owl:ObjectProperty rdf:about="http://www.cs.stevens.edu/cs548#Receive">
      </owl:ObjectProperty>
    </owl:inverseOf>

  </owl:ObjectProperty>

  <owl:Class rdf:about="http://www.cs.stevens.edu/cs548#RadiologistProvided">
    <rdfs:subClassOf rdf:resource="http://www.cs.stevens.edu/cs548#Treatment"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:HasValue rdf:resource="http://www.cs.stevens.edu/cs548#Radiologists"/>
        <owl:OnProperty rdf:resource="http://www.cs.stevens.edu/cs548#ProvidedBy"/>
      </owl:Restriction>
    </owl:equivalentClass>

  </owl:Class>

  <ns0:Patient rdf:about="http://www.cs.stevens.edu/cs548#JaneP">
