              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </rdf:rest>

      </rdf:Description>
    </owl:oneof>

  </owl:Class>

  <owl:Class rdf:about="http://www.cs.stevens.edu/cs548#Provider">
  </owl:Class>

  <owl:Class rdf:about="http://www.cs.stevens.edu/cs548#Patient">
    <owl:hasKey>
      <rdf:Description>
        <rdf:first rdf:resource="http://www.cs.stevens.edu/cs548#PatientId"/>
        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
      </rdf:Description>
    </owl:hasKey>

