    <ns0:Receive rdf:resource="http://www.cs.stevens.edu/cs548#RadiologistProvided"/>
  </rdf:Description>

  <owl:Class rdf:about="http://www.cs.stevens.edu/cs548#Surgery">
  </owl:Class>

